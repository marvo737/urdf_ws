<robot name="SimpleMouse" xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find robot_description)/urdf/body.urdf.xacro" />
    <xacro:include filename="$(find robot_description)/urdf/wheel.urdf.xacro" />
    <xacro:wheel rl="r" parent="body" />
    <xacro:wheel rl="l" parent="body" />
</robot>
